#: addons/lutas_atlanteans.wad/tribes/lutas_atlanteans_tribe/ship/init.lua:142
msgctxt "shipname"
msgid "Tortuga"
msgstr ""

#. TRANSLATORS: This Atlantean ship is named after a mineral
#: addons/lutas_atlanteans.wad/tribes/lutas_atlanteans_tribe/ship/init.lua:144
msgctxt "shipname"
msgid "Tourmaline"
msgstr ""

#. TRANSLATORS: This Atlantean ship is named after an island
#: addons/lutas_atlanteans.wad/tribes/lutas_atlanteans_tribe/ship/init.lua:146
msgctxt "shipname"
msgid "Trinidad"
msgstr ""

#: addons/lutas_atlanteans.wad/tribes/lutas_atlanteans_tribe/tips.lua:4
msgid ""
"Your Temple needs to be built close to water. The priestess who is living "
"there needs a coast to pray."
msgstr ""

#. TRANSLATORS: Format string for a Latin quote and its translation
#: addons/lutas_atlanteans.wad/tribes/lutas_atlanteans_tribe/units.lua:12
