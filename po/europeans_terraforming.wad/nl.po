#: addons/europeans_terraforming.wad/tribes/europeans_tribe/units.lua:1557
msgctxt "building"
msgid "Explores unknown territory."
msgstr "Verkent onbekend terrein."

#. TRANSLATORS: Purpose helptext for a Europeans production site: Terraformer's Hut
#. TRANSLATORS: Purpose helptext for an Europeans production site: Terraformer's House
#: addons/europeans_terraforming.wad/tribes/europeans_tribe/units.lua:1564
#: addons/europeans_terraforming.wad/tribes/europeans_tribe/units.lua:1571
#: addons/europeans_terraforming.wad/tribes/europeans_tribe/units.lua:1578
msgctxt "building"
msgid "Terraforms deserts and coasts into fertile soil."
msgstr "Transformeert woestijnen en kusten tot vruchtbare grond."

#. TRANSLATORS: Purpose helptext for an Europeans production site: Well
#. TRANSLATORS: Purpose helptext for an Europeans production site: Big Well
#: addons/europeans_terraforming.wad/tribes/europeans_tribe/units.lua:1587
#: addons/europeans_terraforming.wad/tribes/europeans_tribe/units.lua:1594
#: addons/europeans_terraforming.wad/tribes/europeans_tribe/units.lua:1601
#: addons/europeans_terraforming.wad/tribes/europeans_tribe/units.lua:1608
