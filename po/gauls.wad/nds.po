#: addons/gauls.wad/scripting/starting_conditions/fortified_village.lua:13
msgid "Start the game with a fortified military installation"
msgstr "Begünn dat Speel mit mennig Soldateree-Anlagen"

#. TRANSLATORS: This is the name of a starting condition
#: addons/gauls.wad/scripting/starting_conditions/headquarters.lua:11
msgid "Headquarters"
msgstr "Raadhuus"

#. TRANSLATORS: This is the tooltip for the "Headquarters" starting condition
#: addons/gauls.wad/scripting/starting_conditions/headquarters.lua:13
msgid "Start the game with your headquarters only"
msgstr "Begünn dat Speel mit nix as dien Raadhuus"

