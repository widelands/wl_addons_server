#: addons/gauls.wad/tribes/gauls_tribe/units.lua:1499
#: addons/gauls.wad/tribes/gauls_tribe/units.lua:1514
#: addons/gauls.wad/tribes/gauls_tribe/units.lua:1546
#: addons/gauls.wad/tribes/gauls_tribe/units.lua:1561
#: addons/gauls.wad/tribes/gauls_tribe/units.lua:1574
#: addons/gauls.wad/tribes/gauls_tribe/units.lua:1620
#: addons/gauls.wad/tribes/gauls_tribe/units.lua:1655
#: addons/gauls.wad/tribes/gauls_tribe/units.lua:1967
