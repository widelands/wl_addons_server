#: addons/europeans_food_production.wad/tribes/europeans_tribe/units.lua:2923
#: addons/europeans_food_production.wad/tribes/europeans_tribe/units.lua:2934
msgctxt "europeans_building"
msgid "Equips the recruits with all necessary armor parts and weapons."
msgstr "Rust de rekruten uit met alle benodigde pantseronderdelen en wapens."

#. TRANSLATORS: Purpose helptext for an Europeans training site: Normal Training Camp, part 1
#. TRANSLATORS: Purpose helptext for an Europeans training site: Advanced Training Camp, part 1
#: addons/europeans_food_production.wad/tribes/europeans_tribe/units.lua:2921
#: addons/europeans_food_production.wad/tribes/europeans_tribe/units.lua:2932
msgctxt "europeans_building"
msgid "Trains recruits to soldiers."
msgstr "Traint rekruten tot soldaten."

#. TRANSLATORS: Note helptext for a Europeans training site: Battle Arena
#: addons/europeans_food_production.wad/tribes/europeans_tribe/units.lua:2942
msgctxt "europeans_building"
msgid "Trains soldiers in ‘Evade’, ‘Health’ and ‘Attack’."
msgstr "Traint soldaten in 'Ontwijken', 'Gezondheid' en 'Aanval'."

#. TRANSLATORS: Purpose helptext for an Europeans training site: Trainingcamp, part 1
#. TRANSLATORS: Purpose helptext for an Europeans training site: Arena, part 1
