#: addons/frisians-economy-ultra.wad/postload.lua:10
msgid "This rye field has been harvested."
msgstr "Deeses Roggfeld is arnt worden."

#: addons/frisians-economy-ultra.wad/postload.lua:13
msgid "This rye field is ready for harvesting."
msgstr "Deeses Roggfeld is klaar tum arnten."

#: addons/frisians-economy-ultra.wad/postload.lua:16
msgid "This rye field is flowering. Beekeepers can use it to produce honey."
msgstr "Deeses Roggfeld blöömt. Imkers könen daar Hönnig mit winnen."

#: addons/frisians-economy-ultra.wad/postload.lua:19
msgid "This rye field is growing."
msgstr "Deeses Roggfeld wasst."

#: addons/frisians-economy-ultra.wad/postload.lua:22
msgid "This rye field has just been planted."
msgstr "Deeses Roggfeld is jüüst plant worden."

#: addons/frisians-economy-ultra.wad/postload.lua:26
msgid "This oat field has been harvested."
msgstr "Deeses Haferfeld is arnt worden."

#: addons/frisians-economy-ultra.wad/postload.lua:29
msgid "This oat field is ready for harvesting."
msgstr "Deeses Haferfeld is klaar tum arnten."

#: addons/frisians-economy-ultra.wad/postload.lua:32
msgid "This oat field is flowering. Beekeepers can use it to produce honey."
msgstr "Deeses Haferfeld blöömt. Imkers könen daar Hönnig mit winnen."

#: addons/frisians-economy-ultra.wad/postload.lua:35
msgid "This oat field is growing."
msgstr "Deeses Haferfeld wasst."

#: addons/frisians-economy-ultra.wad/postload.lua:38
