#: addons/europeans_trading.wad/tradingsites/market_small_craft/init.lua:8
msgctxt "europeans_building"
msgid "Craft products market"
msgstr "Markt voor ambachtelijke producten"

#. TRANSLATORS: This is a building name used in lists of buildings
#: addons/europeans_trading.wad/tradingsites/store_big/init.lua:8
msgctxt "europeans_building"
msgid "Big Store"
msgstr "Grootwarenhuis"

#. TRANSLATORS: This is a building name used in lists of buildings
#: addons/europeans_trading.wad/tradingsites/store_small/init.lua:8
msgctxt "europeans_building"
msgid "Small Store"
msgstr "Kleine Winkel"

#. TRANSLATORS: This is a building name used in lists of buildings
#: addons/europeans_trading.wad/tradingsites/trading_post/init.lua:8
msgctxt "europeans_building"
msgid "Trading Post"
msgstr "Handelspost"

#: addons/europeans_trading.wad/tribes/initialization/init.lua:10
msgid "Mark McWire"
msgstr "Mark McWire"

#. TRANSLATORS: This is a tribe name
#: addons/europeans_trading.wad/tribes/initialization/init.lua:12
msgid "Europeans (Trading)"
msgstr "Europeanen (Handel)"

#: addons/europeans_trading.wad/tribes/initialization/init.lua:13
msgid "This is the culture of the Europeans."
msgstr "Dit is de cultuur van de Europeanen."

#: addons/europeans_trading.wad/tribes/initialization/tips.lua:4
msgid "This is the experimental Europeans tribe."
msgstr "Dit is de experimentele Europese stam."

#. TRANSLATORS: Format string for a latin quote and its translation
#: addons/europeans_trading.wad/tribes/initialization/units.lua:12
msgctxt "europeans_lore"
msgid "‘%1%’<br>(%2%)"
